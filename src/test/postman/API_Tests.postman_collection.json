							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Should return 401 Unauthorized', function() {",
									"    pm.expect(pm.response.code).to.equal(401);",
									"});",
									"",
									"pm.test('Response should indicate authentication required', function() {",
									"    pm.expect(pm.response.status).to.include('Unauthorized');",
